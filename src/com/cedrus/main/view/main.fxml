<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="600.0" prefWidth="660.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cedrus.main.controller.MainController">
   <children>
      <TableView fx:id="customersTable" layoutX="14.0" layoutY="73.0" prefHeight="509.0" prefWidth="387.0" stylesheets="@css/customertable.css">
        <columns>
          <TableColumn fx:id="customerName" editable="false" prefWidth="212.0" />
            <TableColumn fx:id="editCustomer" editable="false" minWidth="0.0" prefWidth="60.0" sortable="false" />
            <TableColumn fx:id="addCustomerHistory" editable="false" minWidth="0.0" sortable="false" />
            <TableColumn fx:id="deleteCustomer" editable="false" minWidth="0.0" prefWidth="32.0" sortable="false" />
        </columns>
      </TableView>
      <HBox fx:id="addNewCustomerHBox" layoutX="290.0" layoutY="50.0" prefHeight="29.0" prefWidth="110.0">
         <padding>
            <Insets top="3.0" />
         </padding></HBox>
      <HBox fx:id="searchCustomerHBox" layoutX="14.0" layoutY="50.0" prefHeight="29.0" prefWidth="208.0">
         <children>
            <ImageView fitHeight="17.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../resources/search.png" />
               </image>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="454.0" layoutY="508.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../resources/cedrus_logo.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
